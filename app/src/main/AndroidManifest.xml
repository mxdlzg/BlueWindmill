<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"

    package="project.mxdlzg.com.bluewindmill">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        tools:replace="android:theme"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:name="project.mxdlzg.com.bluewindmill.view.WindApplication"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="project.mxdlzg.com.bluewindmill.view.activity.MainActivity"
            android:theme="@style/AppTheme.Launcher">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="project.mxdlzg.com.bluewindmill.view.activity.LoginActivity">

        </activity>
        <activity android:name="project.mxdlzg.com.bluewindmill.view.activity.AddScheduleActivity">

        </activity>
        <activity android:name="project.mxdlzg.com.bluewindmill.view.activity.ExamActivity">

        </activity>
        <activity android:name="project.mxdlzg.com.bluewindmill.view.activity.SettingActivity">

        </activity>

        <activity android:name="project.mxdlzg.com.bluewindmill.view.activity.NetTest">

        </activity>
        <activity android:name=".view.activity.ScDetailActivity">

        </activity>
        <activity android:name=".view.activity.ScoreActivity"
            android:theme="@style/AppTheme"/>
        <activity android:name=".view.activity.ScorePointActivity" />
        <activity android:name=".view.activity.ScScoreActivity" />
        <activity android:name=".view.activity.VPNActivity" />
    </application>

</manifest>
